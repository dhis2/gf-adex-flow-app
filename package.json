{
  "name": "gfadex_indicator_upgrader",
  "description": "GF ADEx Indicator Update Tool",
  "version": "0.1.0",
  "dependencies": {
    "@andreasremdt/simple-translator": "^2.0.4"
  },
  "devDependencies": {
    "@dhis2/cli-style": "github:dhis2/cli-style",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "d2-manifest": "^1.0.0",
    "eslint": "^4.11.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browser": {
  "fs": false,
  "path": false,
  "os": false
},
  "main": "./src/app.js",
  "scripts": {
    "build": "webpack --progress",
    "lint": "eslint src",
    "manifest": "d2-manifest package.json build/manifest.webapp",
    "postbuild": "npm run manifest",
    "prebuild": "rm -rf build && npm run lint",
    "start": "d2-manifest package.json manifest.webapp && webpack-dev-server",
    "zip": "rm -rf compiled && npm run build && mkdir compiled && cd build && zip -r ../compiled/$npm_package_name.zip *"
  },
  "manifest.webapp": {
    "developer": {
      "company": "HISP Centre - Univeristy of Oslo",
      "email": "dev@dhis2.org",
      "name": "HISP Centre"
    },
    "icons": {
      "48": "img/icons/app_logo_48x48.png",
      "96": "img/icons/app_logo_96x96.png"
    },
    "name": "GFADEX Upgrade Tool",
    "version": "0.1.0",
    "appType": "APP"
  }
}
